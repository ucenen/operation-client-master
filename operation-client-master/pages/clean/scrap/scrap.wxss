/* pages/clean/scrap/scrap.wxss */
.view1{
  width: 90%;
  height: 230rpx;
  margin-left: 5%;
  border-bottom: 1px solid #eee;
}

.start_time{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}

.select_time{
  width: 60%;
  height: 60rpx;
  margin-left: 30rpx;
  border: 1px solid #eee;
  border-radius:15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #7c7c7c;
}

.view2{
  width: 90%;
  height: 100rpx;
  margin-left: 5%;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  
}


.input1{
  width: 200rpx;
  height: 70rpx;
  border-bottom: 1px solid #eee;
  text-align: center;
  display: flex;
  align-items: center;
}

.but{
  position: fixed;
  bottom: 50rpx;
  width: 90%;
  height: 80rpx;
  margin-left: 5%;
  color: #ffffff;
  background-color: #64c3c7;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 15rpx;
  margin-top: 50rpx;
}