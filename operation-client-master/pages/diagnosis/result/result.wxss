/* pages/diagnosis/result/result.wxss */

.view1{
  width: 90%;
  height: auto;
  margin-top: 30rpx;
  margin-left: 5%;
  border: 1rpx solid #f4f4f4;
  overflow: hidden;
  border-radius: 20rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  margin-bottom: 30rpx;
}

.view2{
  position: fixed;
  bottom:50rpx;
  width: 80%;
  height: 80rpx;
  background-color: #64c3c7;
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 15rpx;
  margin-left: 10%;
}

.title{
  width: 100%;
  height: 80rpx;
  background-color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
}